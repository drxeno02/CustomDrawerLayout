<?xml version="1.0" encoding="utf-8"?>
<resources>
     <declare-styleable name="CustomDrawerLayout">
        <attr name="offsetDistance" format="dimension"/>
        <attr name="stickTo" format="enum">
            <enum name="right" value="3"/>
            <enum name="left" value="2"/>
            <enum name="top" value="4"/>
            <enum name="bottom" value="1"/>
        </attr>

         <attr name="umanoFlingVelocity" format="integer" />
         <attr name="umanoScrollInterpolator" format="reference" />
    </declare-styleable>

</resources>